<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.openimmunizationsoftware.dqa.cm.model">
  <class name="Agreement" table="agreement">
    <id name="agreementId" column="agreement_id">
      <generator class="native"/>
      <!--  native or assigned -->
    </id>
    <property name="agreementTitle" column="agreement_title" type="string"/>
    <property name="agreementText" column="agreement_text" type="string"/>
  </class>
</hibernate-mapping>