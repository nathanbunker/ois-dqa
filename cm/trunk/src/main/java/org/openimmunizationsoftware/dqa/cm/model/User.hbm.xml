<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.openimmunizationsoftware.dqa.cm.model">
  <class name="User" table="user">
    <id name="userId" column="user_id">
      <generator class="native"/>
      <!--  native or assigned -->
    </id>
    <property name="userName" column="user_name" type="string"/>
    <property name="password" column="password" type="string"/>
    <property name="emailAddress" column="email_address" type="string"/>
    <property name="resetPassword" column="reset_password" type="yes_no"/>
    <property name="organization" column="organization" type="string"/>
    <property name="positionTitle" column="position_title" type="string"/>
    <property name="phoneNumber" column="phone_number" type="string"/>
    <property name="adminComments" column="admin_comments" type="string"/>
    <property name="memberTypeString" column="member_type" type="string"/>
  </class>
</hibernate-mapping>